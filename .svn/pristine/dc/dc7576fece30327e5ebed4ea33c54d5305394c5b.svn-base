<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:orientation="vertical"
	android:layout_height="wrap_content" android:background="@drawable/purple_entry_bg"
	android:gravity="center_horizontal|center_vertical" android:minHeight="60dip"
	android:paddingRight="20dip" android:paddingLeft="10dip">
	<ImageView android:id="@+id/leftHead"
		android:layout_width="40dip" android:layout_height="40dip"
		android:src="@drawable/arrow"
		android:layout_gravity="center_horizontal"></ImageView>
	<TextView android:id="@+id/PurpleRowTextView"
		android:gravity="center_horizontal"
		android:layout_height="wrap_content" android:layout_width="80.0dip"
		android:layout_weight="1" android:textSize="12dip"
		android:textColor="@drawable/purple_entry_color">
	    
	</TextView>
	
</LinearLayout>  